{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-console": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules", "build", "scripts", "tests", "dist"]
  }
}
